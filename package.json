{
  "name": "forge",
  "description": "Multi-page markdown document storage and presentation",
  "author": "nivthefox",
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "node": "14.2.x",
    "npm": "6.14.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sosly/forge.git"
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --watch",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "npm run build:prod",
    "start": "ts-node --require tsconfig-paths/register ./src/main.ts",
    "start:dev": "concurrently \"npm run build:dev\" \"ts-node-dev --require tsconfig-paths/register --inspect --respawn --transpileOnly ./src/main.ts\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@koa/router": "^9.0.1",
    "@types/convict": "^5.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/flake-idgen": "^0.1.30",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-session": "^5.10.2",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.0.6",
    "@types/passport-discord": "^0.1.3",
    "@types/reflect-metadata": "^0.1.0",
    "axios": "^0.19.2",
    "convict": "^6.0.0",
    "dotenv": "^8.2.0",
    "flake-idgen": "^1.4.0",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-session": "^6.0.0",
    "koa-static": "^5.0.0",
    "passport-discord": "^0.1.3",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.3",
    "vue": "^2.6.11",
    "vue-router": "^3.0.1",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^5.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "sql.js": "^1.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
