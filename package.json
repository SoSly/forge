{
  "name": "forge",
  "description": "Multi-page markdown document storage and presentation",
  "author": "nivthefox",
  "license": "MIT",
  "version": "1.3.0",
  "engines": {
    "node": ">=16.12.0",
    "yarn": ">=1.22.17"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sosly/forge.git"
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --watch",
    "build:prod": "npm run migrations:run && npm run parser:generate && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "npm run build:prod",
    "dev": "ts-node-dev --require tsconfig-paths/register --inspect --respawn --transpileOnly ./src/main.ts",
    "migrations:clean": "rm ./src/migrations/*.js",
    "migrations:create": "typeorm migration:create",
    "migrations:compile": "tsc -p ./src/migrations",
    "migrations:revert": "typeorm migration:revert",
    "migrations:run": "npm run migrations:compile && typeorm migration:run",
    "parser:generate": "tspeg ./src/usecase/parser/grammar.peg ./src/usecase/parser/Parser.ts",
    "start": "ts-node --require tsconfig-paths/register ./src/main.ts",
    "start:dev": "concurrently \"npm run parser:generate\" \"npm run build:dev\" \"npm run dev\"",
    "typeorm": "typeorm"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@koa/router": "^10.1.1",
    "@types/convict": "^6.1.1",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-session": "^5.10.2",
    "@types/koa-static": "^4.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.4",
    "@types/passport-discord": "^0.1.3",
    "axios": "^0.24.0",
    "convict": "^6.0.0",
    "core-js": "^3.20.1",
    "dotenv": "^10.0.0",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-rewrite": "^3.0.1",
    "koa-session": "^6.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.19",
    "markdown-it": "^12.3.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-toc-done-right": "^4.1.0",
    "passport-discord": "^0.1.3",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.25",
    "typescript": "^4.5.4",
    "uslug": "^1.0.4",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue-drag-drop": "^1.1.4",
    "vue-multipane": "^0.9.5",
    "vue-router": "^3.0.1",
    "vue-shortkey": "^3.1.7",
    "vue2-ace-editor": "0.0.15",
    "vuex": "^3.4.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.6.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^12.4.0",
    "sql.js": "^1.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^5.65.0",
    "webpack-dev-server": "^4.7.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
